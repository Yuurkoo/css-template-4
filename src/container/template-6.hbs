
        <header class="header ">
            <div class="">
                 {{> logo}}
            </div>
            <nav class="header_nav ">
              <div class="header_nav_right">
                {{> nav list=header}}
              </div>
            </nav>
        </header>
      

        <main class="main">
          {{#with info}}
          <div class="info">
            <h1 class="info_title">{{title}}</h1>
            <p class="info_description">{{description}}</p>
          </div>
          {{/with}}

          {{#with form}}
          <form class="form">
            <div class="form_input_list">
              {{#each inputs}}
               <input type="{{type}}" class="form_input" placeholder="{{placeholder}}" name="{{name}}" id="{{id}}">
              {{/each}}
            </div>

            <div class="form_chekbox">
              <input type="checkbox" name="checkbox">
              <label>{{checkbox.label}}</label>
            </div>
            <buttton type="submit" class="form_button">{{button}}</buttton>
            <p class="form_info">{{info}}</p>
          </form>
          {{/with}}
        </main>


