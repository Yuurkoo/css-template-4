      {{#with updates}}
          <div class="updates">
            <h1 class="updates_title">{{title}}</h1>

             {{#each list}}
             <div class="msg">
              <img src="https://picsum.photos/32/32" alt="" class="msg_img">
              <h4 class="msg_nickname">{{nickname}}</h4>
              <p class="msg_description">{{description}}</p>
             </div>
             {{/each}}

            {{#with show_all}}
              <a href="{{url}}" class="messages_link">{{text}}</a>
            {{/with}}
          </div>
          {{/with}}

          {{#with expirience}}
          <div class="expirience">
           <strong class="exp_title">{{title}}</strong>
           <div class="exp_hot">
            <span class="exp_hot_text">
              {{badge}}
            </span>
            </div>
           
           <div class="card">
           {{#each list}}
           <div class="exp_list">
           <h5 class="list_title">{{name}}</h5>
           <p class="list_details">{{details}}</p>
           </div>
           {{/each}}
           </div>
          </div>
          {{/with}}

      <footer class="page_block">  
        <div class="footer">
            <div class="">
               {{> logo}}
               <span class="footer_copyright">Copyright 2023-2024</span>
            </div>

            {{#each footer}}
            <div class="">
                <span class="footer_title">{{title}}</span>
                {{> nav list=links isVertical=true}}
            </div>
            {{/each}}
        </div>
       </footer>
